  // 增强版题干清理函数
  function cleanQuestionText(text) {
      let cleaned = text
          .trim()
          .replace(/^\d+[、.，)]\s*/, '')                     // 去除题号
          .replace(/^(单选题|多选题|判断题)/, '')            // 去除题型前缀
          .replace(/（[A-F、,，\s]*）/, '')                  // 去除括号中的答案提示
          .replace(/[([（][^)\s]*[)\]）]/, '')               // 去除所有括号及其内容
          .replace(/\s+/g, ' ')                              // 多空格归一
          .trim();

      // 递归去除末尾标点
      let lastChar;
      do {
          lastChar = cleaned.slice(-1);
          cleaned = cleaned.replace(/[。？?!.；;，,)）\]）】』】]+$/, '');
      } while (/[。？?!.；;，,)）\]）】』】]/.test(lastChar));

      return cleaned;
  }

  // 改进的答案匹配函数
  function findAnswerKey(questionText) {
      const cleanedText = cleanQuestionText(questionText);

      // 1. 先尝试完全匹配
      for (const key in answerMap) {
          if (cleanQuestionText(key) === cleanedText) {
              return key;
          }
      }

      // 2. 尝试前缀匹配（前30个字符）
      const shortText = cleanedText.slice(0, 15).replace(/[。？?!.；;,\s]/g, '');
      for (const key in answerMap) {
          const shortKey = cleanQuestionText(key).slice(0, 15).replace(/[。？?!.；;,\s]/g, '');
          if (shortText === shortKey) {
              return key;
          }
      }

      return null;
  }

  // 处理多选题答案格式（支持"A,B"、"AC"等格式）
  function parseMultiAnswers(answerStr) {
      return answerStr.split(/[,，\s]*/).filter(Boolean);
  }

  function doAutoAnswer() {
      const questions = document.querySelectorAll('div.test_list');
      if (!questions.length) {
          alert('未找到任何题目');
          return;
      }

      let answeredCount = 0;
      let totalQuestions = questions.length;

      questions.forEach((container, index) => {
          // 获取题干
          let questionText = '';
          let prev = container.previousElementSibling;
          while (prev) {
              if (prev.textContent.trim().length > 6 && !prev.querySelector('p')) {
                  questionText = cleanQuestionText(prev.textContent);
                  break;
              }
              prev = prev.previousElementSibling;
          }

          console.log(`第${index + 1}题 清理后题干: "${questionText}"`);

          if (!questionText) {
              console.warn(`第${index + 1}题 未找到题干`);
              return;
          }

          // 获取题目类型
          const questionTypeInput = container.querySelector('input[name="quesType"]');
          const isMultiChoice = questionTypeInput && questionTypeInput.value === "2";

          console.log(`第${index + 1}题 题型: ${isMultiChoice ? '多选题' : '单选题'}`);

          const matchedKey = findAnswerKey(questionText);
          if (!matchedKey) {
              console.warn(`第${index + 1}题 未找到匹配答案`);
              return;
          }

          const correctAnswers = answerMap[matchedKey].trim();
          console.log(`第${index + 1}题 匹配答案: "${correctAnswers}"`);

          const options = container.querySelectorAll('p[id]');
          const matchedOptions = [];

          options.forEach(p => {
              const spans = p.querySelectorAll('span');
              if (spans.length < 2) return;

              const optionLetter = spans[0].innerText.trim();

              if (isMultiChoice) {
                  // 多选题匹配逻辑
                  const answerLetters = parseMultiAnswers(correctAnswers);
                  if (answerLetters.includes(optionLetter)) {
                      matchedOptions.push(p);
                  }
              } else {
                  // 单选题匹配逻辑
                  if (correctAnswers.includes(optionLetter)) {
                      matchedOptions.push(p);
                  }
              }
          });

          if (matchedOptions.length > 0) {
              matchedOptions.forEach(p => {
                  // 清除可能存在的旧样式
                  p.style.backgroundColor = '';
                  p.style.borderLeft = '';

                  // 执行点击操作
                  const clickEvent = new MouseEvent('click', {
                      view: window,
                      bubbles: true,
                      cancelable: true
                  });
                  p.dispatchEvent(clickEvent);

                  // 添加新样式
                  p.style.backgroundColor = '#d4edda';
                  p.style.borderLeft = '3px solid #28a745';
                  p.style.paddingLeft = '10px';
                  p.style.transition = 'all 0.3s ease';
              });

              answeredCount++;
              console.log(`✅ 第${index + 1}题 已选择 ${matchedOptions.length} 个选项`);
          } else {
              console.warn(`第${index + 1}题 未匹配到任何选项`);
          }
      });

      // 显示答题结果
      const resultMsg = `已完成答题: ${answeredCount}/${totalQuestions}`;
      console.log(resultMsg);
      alert(resultMsg);
  }

  function addButton() {
      // 移除已存在的按钮
      const oldBtn = document.querySelector('#autoAnswerBtn');
      if (oldBtn) oldBtn.remove();

      const btn = document.createElement('button');
      btn.id = 'autoAnswerBtn';
      btn.textContent = '开始自动答题';
      btn.style.position = 'fixed';
      btn.style.bottom = '20px';
      btn.style.right = '200px';
      btn.style.zIndex = '99999';
      btn.style.padding = '50px 50px';
      btn.style.backgroundColor = '#28a745';
      btn.style.color = 'white';
      btn.style.border = 'none';
      btn.style.borderRadius = '5px';
      btn.style.fontSize = '16px';
      btn.style.fontWeight = 'bold';
      btn.style.boxShadow = '0 2px 10px rgba(0,0,0,0.2)';
      btn.style.cursor = 'pointer';
      btn.title = '点击自动选择答案';

      btn.addEventListener('click', () => {
          btn.disabled = true;
          btn.textContent = '答题中...';
          btn.style.backgroundColor = '#6c757d';

          setTimeout(() => {
              try {
                  doAutoAnswer();
              } catch (e) {
                  console.error('自动答题出错:', e);
                  alert('自动答题出错，请查看控制台');
              }

              btn.textContent = '重新答题';
              btn.style.backgroundColor = '#28a745';
              btn.disabled = false;
          }, 500);
      });

      document.body.appendChild(btn);
  }

  // 页面加载完成后添加按钮
  if (document.readyState === 'complete') {
      addButton();
  } else {
      window.addEventListener('load', addButton);
  }

  // 添加样式美化选中效果
  const style = document.createElement('style');
  style.textContent = `
      p[id$="A"], p[id$="B"], p[id$="C"], p[id$="D"] {
          transition: all 0.3s ease;
      }
      p[id$="A"]:hover, p[id$="B"]:hover, p[id$="C"]:hover, p[id$="D"]:hover {
          transform: translateX(5px);
      }
  `;
  document.head.appendChild(style);
})();
